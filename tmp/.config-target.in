choice
	prompt "Target System"
	default TARGET_ar71xx
	reset if !DEVEL
	
config TARGET_ar71xx
	bool "Atheros AR7xxx/AR9xxx"
	select HAS_SUBTARGETS
	select mips
	help
	  Build firmware images for Atheros AR7xxx/AR9xxx based boards.


endchoice

choice
	prompt "Subtarget" if HAS_SUBTARGETS

config TARGET_ar71xx_generic
	bool "Generic"
	select LINUX_3_3
	depends TARGET_ar71xx
	select GPIO_SUPPORT
	select USES_JFFS2
	select PCI_SUPPORT
	select USES_SQUASHFS
	select USES_TARGZ
	select USB_SUPPORT
	select mips
	help
	  Build firmware images for Atheros AR7xxx/AR9xxx based boards.


